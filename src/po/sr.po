msgstr "Поништи у_мањење"

#: ../libwnck/window-action-menu.c:420
msgid "Mi_nimize"
msgstr "_Умањи"

#: ../libwnck/window-action-menu.c:428
msgid "Unma_ximize"
msgstr "Поништи у_већање"

#: ../libwnck/window-action-menu.c:435
